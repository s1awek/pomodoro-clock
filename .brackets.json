{
    "path": {
        "scss/style.scss": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css",
                "imagePath": null,
                "sourceComments": false,
                "outputStyle": "compact"
            },
            "me.drewh.jsbeautify.on_save": false,
            "brackets-git.gitPath": "git",
            "linting.collapsed": false
        },
        "includes/*.scss": {
            "sass.enabled": false,
            "linting.collapsed": true,
            "brackets-git.gitPath": "git"
        }
    }
}
